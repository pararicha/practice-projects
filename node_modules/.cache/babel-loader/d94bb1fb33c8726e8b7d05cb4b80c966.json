box){\n            if(box === 'A'){\n            this.boxSelectedA = true;\n            }\n            else if(box === \"B\" ){\n                this.BoxSelectedB = true;\n            }\n            else\n            this.BoxSelectedC = true;\n        },\n        outputGoal(){\n            const randomNumber = Math.random();\n            if(randomNumber < 0.5){\n                return \"Learn vue\";\n            }else{\n                return \"Master vue\";\n            }\n            \n        },\n        add(num){\n            this.counter = this.counter+num;\n            return this.counter;\n\n        },\n        setName(event){\n            this.name = event.target.value ;\n        },\n        submitForm(){\n            alert(\"successful!\");\n        },\n        resetInput(){\n            this.name = \"\";\n        }\n\n    }\n};\n</script>\n    "],"sourceRoot":"src/components"}]}