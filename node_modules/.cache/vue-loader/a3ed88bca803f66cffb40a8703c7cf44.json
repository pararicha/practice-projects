>\n{{fullName}}\n{{firstName}}\n{{lastName}}\n<div v-bind:style=\"styleObject\">\n  <p v-if=\"awesome\">I am richa </p>\n  <p v-else>\"oh no\"</p>\n</div>\n<div v-if=\"loginType === 'username'\">\n  <label>Username</label>\n  <input placeholder=\"Enter your username\">\n</div>\n<div v-else>\n  <label>Email</label>\n  <input placeholder=\"Enter your email address\">\n</div>\n<p>{{obj.foo}}</p>\n<button v-on:click=\"obj.foo='baz'\">change it</button>\n\n<div v-bind:id=\"dynamicId\">\n  {{dynamicId}}\n</div>\n<p v-if=\"see\" style=\"color:red;font-weight:bold;text-align:left\">I am feeling uneasy in my right hand</p>\n<a v-on:[eventName]=\"doSomething\">i am ready</a>{{a}}\n<a v-bind:[attributeName]=\"styleObject\"> I am a link </a>\n<div id=\"example\">\n  {{abc.split('').reverse('').join('')}}\n</div>\n<input ref=\"item\"/>\n<button v-on:click=\"handleSubmit\">Add to the list</button>\n<div class=\"first-child\">\n<h3>Form User</h3>\n    <div class=\"form-group\">\n      <label for=\"usr\">Name:</label>\n      <input type=\"text\" class=\"form-control\" v-model=\"name\">\n    </div>\n    <div class=\"form-group\">\n      <label for=\"pwd\">Password:</label>\n      <input type=\"password\" class=\"form-control\" v-model=\"password\">\n    </div>\n    <div class=\"form-group\">\n      <label for=\"pwd\">Email:</label>\n      <input type=\"text\" class=\"form-control\" v-model=\"email\">\n      \n    </div>\n    {{name}}\n  </div>\n</div>\n\n\n</template>\n\n<script>\n//import HelloWorld from './components/HelloWorld.vue'\n// import Sort from './components/Sort.vue'\n// import Abc from './components/Abc.vue'\nimport todoItem from './components/todo.vue'\n\nexport default {\n  name: 'App',\n  data() {\n        return{\n          obj :({foo:'bar'}),\n    groceryList: [\n      { id: 0, text: 'Vegetables' },\n      { id: 1, text: 'Cheese' },\n      { id: 2, text: 'Whatever else humans are supposed to eat' },\n      \n    ],\n    rawHtml : '<span style=\"color:red ;font-style:italic ;font-weight:bold\">Hi i am happy </span>',\n    isButtonDisabled: true,\n    message:'richa',\n    seen: true,\n    abc:'richa',\n    a:10,\n    b:40,\n    see:true,\n    name:'fghj',\n      password:'',\n      email:'',\n    dynamicId:\"i am studying\",\n    msg:\"I am richa Parashar\",\n    firstName :\"Richa\",\n    lastName :\"Parashar\",\n    isActive: true,\n    attributeName:\"style\",\n    url:\"http://www.google.com\",\n    eventName:\"click\",\n  error: true,\n  styleObject:{\n    color: 'blue',\n    fontSize: '13px',\n    fontWeight:'bold',\n    \n  },\n  awesome:false,\n   ok:false,\n   loginType:\"address\",\n\n\n    }\n  },\n  \n  \n  methods : {\n   \n    greeting : function(){\n        this.loginType = 'username';\n         },\n  \n\n    sum : function(){\n      return this.a+this.b;\n    },\n    doSomething: function(){\n      this.a = this.a + 10;\n      //console.log(\"a is \" +this.a);\n\n    },\n    example: function(){\n      return this.abc;\n    }\n  },\n  computed: {\n  fullName: {\n    // getter\n    get: function () {\n      return this.firstName + ' ' + this.lastName\n    },\n    // setter\n    set: function (newValue) {\n      var names = newValue.split(' ')\n      this.firstName = names[0]\n      this.lastName = names[names.length - 1]\n    }\n  },\n  classObject: function () {\n    //this.error.type='fatal'\n    return {\n      active: this.isActive,\n      'text-danger': this.error\n    }\n  }\n},\n \n\n    \n  \n  \n  components: {\n      todoItem,\n}\n      \n}\n</script>\n\n<style scopr>\n#sort {\n  font-family: Avenir, Helvetica, Arial, sans-serif;\n  -webkit-font-smoothing: antialiased;\n  -moz-osx-font-smoothing: grayscale;\n  text-align: center;\n  color: #2c3e50;\n  margin-top: 60px;\n}\n.active {\nfont-family: Avenir, Helvetica, Arial, sans-serif;\n  -webkit-font-smoothing: antialiased;\n  -moz-osx-font-smoothing: grayscale;\n  text-align: center;\n  font-weight: bold;\n  color:red;\n  margin-top: 60px;\n}\n.text-danger{\n  font-family: Avenir, Helvetica, Arial, sans-serif;\n  -webkit-font-smoothing: antialiased;\n  -moz-osx-font-smoothing: grayscale;\n  text-align: center;\n  font-style: italic;\n  color: green;\n  margin-top: 60px;\n}\n.first-child{\nmargin: 0 auto;\nmax-width: 600px;\n}\n\n</style>\n"]}]}